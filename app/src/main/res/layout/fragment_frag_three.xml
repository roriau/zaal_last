<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/md_white_1000"
    android:theme="@style/Theme.MaterialComponents.Light.Bridge"
    tools:context=".FragThree">

    <ImageView
        android:id="@+id/emptyImage"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:src="@drawable/empty_cart"
        android:layout_centerInParent="true"
        android:layout_marginStart="10dp"
        android:layout_marginEnd="10dp"> </ImageView>

    <com.google.android.material.button.MaterialButton
        android:id="@+id/loginButton"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@+id/emptyImage"
        android:text="ЭНД ДАРЖ НЭВТЭРНЭ ҮҮ"
        android:textColor="@color/md_white_1000"
        android:gravity="center"
        android:layout_marginStart="25dp"
        android:layout_marginEnd="25dp"
        app:backgroundTint="@color/primary_dark"
        app:icon="@drawable/ic_account_circle"
        app:iconGravity="textStart"
        app:iconSize="18dp"
        android:padding="10dp" > </com.google.android.material.button.MaterialButton>


</RelativeLayout>